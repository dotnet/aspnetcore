{
  "Default": {
    "Client": "SignalR",
    "Source": {
      "Repository": "https://github.com/aspnet/aspnetcore.git",
      "BranchOrCommit": "master",
      "Project": "src/SignalR/benchmarkapps/BenchmarkServer/BenchmarkServer.csproj"
    },
    "Connections": 10,
    "Duration": 20,
    "Warmup": 2,
    "Path": "/echo",
    "ClientProperties": {
      "CollectLatency": "true"
    }
  },
  "SignalRBroadcast": {
    "ClientProperties": { "Scenario": "broadcast" }
  },
  "SignalREcho": {
    "ClientProperties": { "Scenario": "echo" }
  },
  "SignalREchoAll": {
    "ClientProperties": { "Scenario": "echoAll" },
    "Warmup": 0
  },
  "SignalREchoIdle": {
    "ClientProperties": {
      "Scenario": "echoIdle",
      "CollectLatency": "false"
    }
  }
}
