<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\..\Shared\Process\ProcessEx.cs" Link="Helpers\ProcessEx.cs" />
    <Compile Include="..\..\..\Shared\Process\ProcessExtensions.cs" Link="Helpers\ProcessExtensions.cs" />
  </ItemGroup>

  <PropertyGroup>
    <InteropTestsAssestsDir>$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory)..\, Directory.Build.props))\src\Grpc\test\testassets</InteropTestsAssestsDir>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>InteropTestsWebsiteDir</_Parameter1>
      <_Parameter2>$(InteropTestsAssestsDir)\InteropWebsite</_Parameter2>
    </AssemblyAttribute>
    
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadataAttribute">
      <_Parameter1>InteropTestsClientDir</_Parameter1>
      <_Parameter2>$(InteropTestsAssestsDir)\InteropClient</_Parameter2>
    </AssemblyAttribute>
  </ItemGroup>

</Project>
