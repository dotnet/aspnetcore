    <Reference Include="Microsoft.AspNetCore.Server.Kestrel.Core" />
    <Reference Include="Microsoft.AspNetCore.Server.Kestrel.Https" />
    <Reference Include="Microsoft.AspNetCore.Server.Kestrel.Transport.Libuv" />
