{
  "version": "0.2.0-*",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "description": "A compatibility layer for sharing identity databases between Microsoft.AspNet.Identity.EntityFramework and Microsoft.AspNetCore.Identity.EntityFrameworkCore.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/identity"
    },
    "tags": [
      "aspnetcore",
      "identity",
      "membership"
    ]
  },
  "dependencies": {
    "Microsoft.AspNet.Identity.EntityFramework": "2.2.1",
    "Microsoft.Owin.Security.Cookies": "3.0.1",
    "Microsoft.Owin.Security.Interop": "1.2.0-*",
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Data": ""
      }
    }
  }
}