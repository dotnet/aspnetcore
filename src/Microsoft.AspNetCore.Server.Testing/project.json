{
  "version": "1.0.0-*",
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "description": "ASP.NET 5 helpers to deploy applications to IIS Express, IIS, WebListener and Kestrel for testing.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/hosting"
  },
  "dependencies": {
    "Microsoft.AspNetCore.Testing": "1.0.0-*",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.0-*",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-*",
    "Microsoft.Extensions.Process.Sources": {
      "type": "build",
      "version": "1.0.0-*"
    }
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "Microsoft.Web.Administration": "7.0.0"
      },
      "frameworkAssemblies": {
        "System.Net.Http": "",
        "System.Runtime":  { "type": "build" }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Diagnostics.Process": "4.1.0-*",
        "System.IO.FileSystem": "4.0.1-*",
        "System.Net.Http": "4.0.1-*",
        "System.Net.Primitives": "4.0.11-*",
        "System.Net.Sockets": "4.1.0-*",
        "System.Runtime.Extensions": "4.1.0-*",
        "System.Text.RegularExpressions": "4.0.12-*",
        "System.Threading": "4.0.11-*",
        "System.Threading.Thread": "4.0.0-*"
      },
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}