  },
  "dependencies": {
    "Microsoft.AspNetCore.Authentication": "1.0.0-*",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {
    }
  }
}