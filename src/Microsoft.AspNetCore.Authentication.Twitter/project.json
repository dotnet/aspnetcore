{
  "version": "1.0.0-*",
  "description": "ASP.NET Core middleware that enables an application to support Twitter's OAuth 2.0 authentication workflow.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/security"
    },
    "tags": [
      "aspnetcore",
      "authentication",
      "security"
    ]
  },
  "buildOptions": {
