{
  "description": "The Razor view engine for ASP.NET MVC.",
  "version": "1.0.0-*",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/mvc"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Razor.Host": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc.ViewFeatures": "1.0.0-*",
    "Microsoft.CodeAnalysis.CSharp": "1.1.0-rc1-20151109-01",
    "Microsoft.Extensions.FileProviders.Composite": "1.0.0-*",
    "Microsoft.Extensions.HashCodeCombiner.Sources": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "Microsoft.Extensions.PropertyActivator.Sources": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "Microsoft.Extensions.PropertyHelper.Sources": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "System.Collections": "4.0.11-*",
    "System.IO": "4.1.0-*",
    "System.Reflection": "4.1.0-*",
    "System.Threading": "4.0.11-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Threading.Tasks": { "type": "build" }
      }
    },
    "netstandard1.5": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ],
      "dependencies": {
        "System.Text.Encoding": "4.0.11-*",
        "System.Threading.Tasks.Parallel": "4.0.1-*",
        "System.Runtime.Loader": "4.0.0-*"
      }
    }
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}