{
  "version": "1.0.0-*",
  "buildOptions": {
    "preserveCompilationContext": true,
    "emitEntryPoint": true
  },
  "packOptions": {
    "files": {
      "mappings": {
        "lib/": "obj/precompiled/"
      }
    }
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc": "1.2.0-*",
    "Microsoft.AspNetCore.Mvc.Razor.ViewCompilation.Design": {
      "version": "1.2.0-*",
      "target": "package",
      "type": "build"
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Mvc.Razor.ViewCompilation.Tools": "1.2.0-*"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.2.0-*",
          "type": "platform"
        }
      }
    },
    "net451": {}
  }
}